# マネーフォワードのログイン情報
EMAIL=your_email@example.com
PASSWORD=your_password

# マネーフォワードの二段階認証シークレットキー
# 二段階認証を設定する際に表示されるシークレットキー（QRコードの下に表示される文字列）
# 注意: スペースは自動的に削除されますが、できるだけスペースなしで設定してください
# 例: JBSWY3DPEHPK3PXP
TOTP_SECRET=your_totp_secret_key

# Notion API
NOTION_KEY=your_notion_token
NOTION_PAGE_ID=your_notion_page_id

# LINE Messaging API
LINE_ACCESS_PARSE_MONEY_FORWORD_TOKEN=your_line_access_token
LINE_ACCESS_LOG_RELAY_TOKEN=your_line_log_relay_token
USER_ID=your_line_user_id

# 各種金額設定
HOUSE_BANK=0
RAKUTEN_BANK=0
HOUSE_RENT=0
FIXED_COST=0
FOOD_EXPENSE=0
